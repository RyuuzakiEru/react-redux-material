{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb", "plugin:jest/recommended","prettier","no-unused-expressions"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react", "jest", "prettier"],
  "rules": {
      "prettier": true,
      "react/jsx-filename-extension": [0],
      "react/prop-types": 0,
      "react/destructuring-assignment": 0,
      "no-underscore-dangle": 0,
      "react/jsx-wrap-multilines": 0,
      "no-case-declarations": 0,
      "consistent-return" : 0
    }
}
